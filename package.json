{
  "name": "@yrnana/vite-react-lib",
  "description": "Vite-Based React Component Library Template",
  "version": "0.0.4",
  "author": "Yuri Na <nyryn0945@gmail.com> (https://yrnana.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/yrnana/vite-react-lib.git"
  },
  "homepage": "https://github.com/yrnana/vite-react-lib#readme",
  "bugs": {
    "url": "https://github.com/yrnana/vite-react-lib/issues"
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --fix",
    "type-check": "tsc -p tsconfig.json",
    "format": "prettier --write \"./src/**/*.{js,ts,tsx,scss,json,html}\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "yarn build && changeset publish"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@emotion/babel-plugin": "^11.9.2",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-react": "^1.3.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.6",
    "vite-plugin-dts": "^1.1.1"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ],
    "production": [
      "defaults",
      "not IE 11"
    ]
  },
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
